#include <stdio.h>
int main() {
   int year,month,day;
   printf("Enter a dd/mm/yy: ");
   scanf("%d/%d/%d",&day,&month,&year);
   
   if (year % 4 == 0) 
   {
      printf("%d is a leap year.", year);
   }
   else if (year % 100 == 0)
   {
      printf("%d is a leap year.", year);
   }
   
   else if (year % 400 == 0) {
      printf("%d is a leap year.", year);
   }
   else
   {
      printf("%d is not a leap year.", year);
   }
   return 0;
}
